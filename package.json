{
	"name": "directus-extension-kubernetes-endpoint",
	"description": "Please enter a description for your extension",
	"icon": "extension",
	"version": "1.0.0",
	"keywords": [
		"directus",
		"directus-extension",
		"directus-custom-endpoint"
	],
	"type": "module",
	"directus:extension": {
		"type": "endpoint",
		"path": "src/index.js",
		"source": "src/index.js",
		"host": "^10.3.4"
	},
	"scripts": {
		"build": "directus-extension build",
		"dev": "directus-extension build -w --no-minify",
		"link": "directus-extension link",
		"format": "prettier -w src/"
	},
	"devDependencies": {
		"@directus/extensions-sdk": "10.3.4",
		"prettier": "^3.2.5"
	},
	"dependencies": {
		"@kubernetes/client-node": "0.18.1",
		"kitegg-directus-extension-common": "github:KITeGG-Node-Modules/kitegg-directus-extension-common",
		"luxon": "^3.4.4",
		"slugify": "^1.6.6",
		"validate.js": "^0.13.1",
		"yaml": "^2.4.1"
	}
}
